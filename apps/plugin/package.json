{
	"name": "@leat/plugin",
	"version": "0.9.71",
	"description": "Leat WordPress plugin",
	"author": "Leat",
	"homepage": "https://leat.com",
	"contributors": [
		{
			"name": "Leat",
			"email": "support@leat.com",
			"url": "https://leat.com"
		},
		{
			"name": "Chris Jayden",
			"email": "hi@chrisjayden.com",
			"url": "https://chrisjayden.com"
		}
	],
	"private": true,
	"type": "module",
	"scripts": {
		"clean": "rm -rf .turbo node_modules",
		"dev": "concurrently \"pnpm dev:frontend\" \"pnpm dev:admin\"",
		"dev:frontend": "NODE_ENV=development vite --config vite-frontend.config.ts --port 4001",
		"dev:admin": "NODE_ENV=development vite --config vite-admin.config.ts --port 4002",
		"build": "pnpm build:frontend && pnpm build:admin",
		"build:frontend": "NODE_ENV=production vite build --config vite-frontend.config.ts",
		"build:admin": "NODE_ENV=production vite build --config vite-admin.config.ts",
		"analyze": "ANALYZE=true pnpm build",
		"i18n": "php -d memory_limit=4096M $(which wp) i18n --allow-root make-pot . languages/leat.pot --slug=leat-crm --domain=leat-crm --include=src,dist/admin/assets,dist/frontend/assets --exclude=bin,node_modules,tests,vendor,dist/admin/assets/codemirror-*.js,dist/admin/assets/vendor-*.js,dist/admin/assets/*.js.map",
		"typecheck": "svelte-check --tsconfig ./tsconfig.json",
		"typecheck:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"package-plugin": "rimraf leat.zip && ./bin/build-plugin-zip.sh",
		"package-plugin:dev": "rimraf leat.zip && ./bin/build-plugin-zip.sh -d",
		"package-plugin:zip-only": "rimraf leat.zip && ./bin/build-plugin-zip.sh -z",
		"package-plugin:ci": "rimraf leat.zip && ./bin/build-plugin-zip.sh -k",
		"lint-php": "composer run-script phpcs ./src",
		"lint-php-fix": "composer run-script phpcbf ./src",
		"lint": "eslint \"./src/**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
		"format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json,svelte}\"",
		"env:start": "wp-env start",
		"env:stop": "wp-env stop",
		"env:reset": "wp-env reset",
		"env:clean": "wp-env clean all && wp-env start",
		"env:destroy": "wp-env destroy",
		"env:restart": "wp-env start --update"
	},
	"devDependencies": {
		"@codemirror/autocomplete": "^6.18.4",
		"@codemirror/lang-css": "^6.3.1",
		"@codemirror/lang-javascript": "^6.2.2",
		"@codemirror/language": "^6.10.8",
		"@codemirror/lint": "^6.8.4",
		"@codemirror/state": "^6.5.1",
		"@codemirror/theme-one-dark": "^6.1.2",
		"@codemirror/view": "^6.36.2",
		"@kucrut/vite-for-wp": "^0.10.0",
		"@leat/lib": "workspace:^",
		"@leat/plugin-admin": "workspace:^",
		"@leat/plugin-frontend": "workspace:^",
		"@leat/tailwind-config": "workspace:^",
		"@leat/types": "workspace:*",
		"@popperjs/core": "^2.11.8",
		"@rgossiaux/svelte-headlessui": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^2.5.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tanstack/svelte-query-devtools": "^5.64.0",
		"@tsconfig/svelte": "^5.0.4",
		"@types/dinero.js": "^1.9.4",
		"@types/jquery": "^3.5.32",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^18.19.70",
		"@types/qs": "^6.9.17",
		"@wordpress/env": "^9.10.0",
		"@wordpress/hooks": "^3.58.0",
		"@wordpress/i18n": "^4.58.0",
		"@wordpress/keycodes": "^3.58.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.5.7",
		"codemirror": "^6.0.1",
		"concurrently": "^8.2.2",
		"dinero.js": "^1.9.1",
		"install": "^0.13.0",
		"lodash-es": "^4.17.21",
		"lucide-svelte": "^0.288.0",
		"postcss": "^8.4.49",
		"qs": "^6.13.1",
		"rimraf": "^5.0.10",
		"rollup-plugin-external-globals": "^0.13.0",
		"rollup-plugin-license": "^3.5.3",
		"rollup-plugin-visualizer": "^5.14.0",
		"svelte": "^4.2.19",
		"svelte-bootstrap-icons": "^3.1.2",
		"svelte-check": "^3.8.6",
		"svelte-codemirror-editor": "^1.4.1",
		"svelte-multiselect": "^10.3.0",
		"svelte-portal": "^2.2.1",
		"svelte-preprocess": "^5.1.4",
		"svelte-tippy": "^1.3.2",
		"svelte-transition": "^0.0.10",
		"svelte-writable-derived": "^3.1.1",
		"tailwindcss": "^3.4.17",
		"tippy.js": "^6.3.7",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vanilla-colorful": "^0.7.2",
		"vite": "^4.5.5",
		"vite-imagetools": "^6.2.9",
		"vite-plugin-banner": "^0.7.1",
		"vite-plugin-external": "^4.3.1"
	},
	"dependencies": {
		"@splidejs/svelte-splide": "^0.2.9",
		"@tanstack/svelte-query": "^5.64.0",
		"@wordpress/a11y": "^3.58.0",
		"@wordpress/html-entities": "^3.58.0",
		"history": "^5.3.0",
		"slugify": "^1.6.6",
		"svelte-local-storage-store": "^0.6.4",
		"svelte-navigator": "^3.2.2"
	}
}