{
	"$schema": "https://turbo.build/schema.v2.json",
	"tasks": {
		"topo": {
			"dependsOn": [
				"^topo"
			]
		},
		"build": {
			"dependsOn": [
				"^build"
			],
			"outputs": [
				"dist/**"
			]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"sync": {
			"cache": false
		},
		"dev:admin": {
			"persistent": true,
			"cache": false
		},
		"dev:frontend": {
			"persistent": true,
			"cache": false
		},
		"analyze": {
			"dependsOn": [
				"^build"
			]
		},
		"@leat/plugin#i18n": {
			"cache": false,
			"dependsOn": [
				"^build",
				"@leat/plugin#package-plugin"
			],
			"outputs": [
				"languages/**"
			]
		},
		"@leat/plugin#package-plugin": {
			"dependsOn": [
				"build"
			],
			"outputs": [
				"./leat-crm.zip"
			],
			"inputs": [
				"dist/**",
				"src/**",
				"vendor/**",
				"leat-crm.php"
			]
		},
		"@leat/plugin#package-plugin:ci": {
			"dependsOn": [
				"build"
			],
			"outputs": [
				"dist/**",
				"./leat-crm.zip"
			],
			"inputs": [
				"dist/**",
				"src/**",
				"vendor/**",
				"leat-crm.php"
			]
		},
		"format": {
			"outputs": [
				"node_modules/.cache/.prettiercache"
			]
		},
		"lint": {
			"dependsOn": [
				"^topo"
			],
			"outputs": [
				"node_modules/.cache/.eslintcache"
			]
		},
		"typecheck": {
			"dependsOn": [
				"^topo"
			],
			"outputs": [
				"node_modules/.cache/tsbuildinfo.json"
			]
		},
		"clean": {
			"cache": false
		},
		"//#clean": {
			"cache": false
		}
	}
}