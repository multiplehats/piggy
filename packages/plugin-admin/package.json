{
	"name": "@leat/plugin-admin",
	"homepage": "https://leat.com",
	"author": "Chris Jayden",
	"private": true,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"clean": "rm -rf .turbo node_modules",
		"dev:admin": "NODE_ENV=development svelte-package --watch",
		"build": "NODE_ENV=production pnpm sync && svelte-package",
		"sync": "svelte-kit sync",
		"analyze": "ANALYZE=true pnpm build",
		"typecheck": "svelte-check --tsconfig ./tsconfig.json",
		"typecheck:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint \"./src/**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
		"format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json,svelte}\"",
		"ui:add": "pnpx shadcn-svelte@latest add"
	},
	"devDependencies": {
		"@leat/lib": "workspace:^",
		"@leat/tailwind-config": "workspace:^",
		"@leat/types": "workspace:*",
		"@sveltejs/kit": "^2.15.2",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/forms": "^0.5.10",
		"@tanstack/svelte-query-devtools": "^5.64.0",
		"@tsconfig/svelte": "^5.0.4",
		"@types/jquery": "^3.5.32",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^18.19.70",
		"@wordpress/env": "^9.10.0",
		"@wordpress/hooks": "^3.58.0",
		"@wordpress/i18n": "^4.58.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^0.22.0",
		"clsx": "^2.1.1",
		"lodash-es": "^4.17.21",
		"lucide-svelte": "^0.378.0",
		"postcss": "^8.4.49",
		"svelte": "^5.19.5",
		"svelte-check": "^4.1.4",
		"tailwind-merge": "^1.14.0",
		"tailwind-variants": "^0.1.20",
		"tailwindcss": "^3.4.17",
		"tslib": "^2.8.1",
		"typescript": "^5.7.3",
		"vanilla-colorful": "^0.7.2",
		"vite": "^6.0.11"
	},
	"dependencies": {
		"@internationalized/date": "^3.6.0",
		"@tanstack/svelte-query": "^5.65.0",
		"@wordpress/a11y": "^3.58.0",
		"cmdk-sv": "^0.0.17",
		"history": "^5.3.0",
		"svelte-navigator": "^3.2.2",
		"zod": "^3.24.1"
	}
}